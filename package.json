{
  "author": "dark-energy",
  "name": "webgl-particles",
  "version": "0.0.2",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "cross-env": "^3.2.4",
    "html-webpack-plugin": "^2.26.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "description": "my particles editor",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "devserver": "webpack-dev-server --watch --port 8000 --env.debug --config webpack.lib.js",
    "dev": "webpack --progress --watch --env.debug --config webpack.lib.js",
    "deved": "webpack --progress --watch --env.debug --config webpack.editor.js",
    "build": "webpack --watch --config webpack.libprod.js",
    "editordev":   "webpack --progress --watch --env.debug --config webpack.editor.dev.js",
    "editor": "webpack --progress --hide-modules --config webpack.editor.prod.js",    
    "enginedev": "webpack --watch --progress --config webpack.engine.dev.js",
    "engineprod": "webpack --progress --hide-modules --config webpack.engine.prod.js",    
    "guidev": "webpack --watch --env.debug --progress --config webpack.gui.dev.js",
    "guiprod": "webpack --env.debug --config webpack.gui.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dark-Energy/webgl_particles.git"
  },
  "keywords": [
    "webgl",
    "3d",
    "shaders",
    "gpu",
    "particles"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dark-Energy/webgl_particles/issues"
  },
  "homepage": "https://github.com/Dark-Energy/webgl_particles#readme",
  "dependencies": {
    "uglifyjs-webpack-plugin": "^0.4.6"
  }
}
